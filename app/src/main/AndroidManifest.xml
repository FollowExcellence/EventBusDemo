<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sum.eventbusdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".normal.OneActivity"/>
        <activity android:name=".normal.TwoActivity"/>
        <activity android:name=".sticky.StickySendActivity"/>
        <activity android:name=".sticky.StickyReceiveActivity"/>
        <activity android:name=".androidbus.AndroidBusSendActivity"/>
        <activity android:name=".androidbus.AndroidBusReceiveActivity"/>
        <activity android:name=".priority.PrioritySendActivity"/>
        <activity android:name=".priority.PriorityReceiveActivity"/>
        <activity android:name=".threadmode.ModeSendActivity"/>
        <activity android:name=".threadmode.ModeReceiveActivity"/>
        <activity android:name=".androidsticky.AndroidStickySendActivity"/>
        <activity android:name=".androidsticky.AndroidStickyReceiveActivity"/>
    </application>

</manifest>